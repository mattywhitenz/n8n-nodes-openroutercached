{
  "name": "n8n-nodes-openroutercached",
  "version": "0.4.3",
  "description": "n8n node for OpenRouter API integration with advanced features: prompt caching, reasoning tokens, vision, streaming, and more",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "openrouter",
    "ai",
    "llm",
    "prompt-caching",
    "reasoning-tokens",
    "vision",
    "multimodal"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mattywhitenz/n8n-nodes-openroutercached",
  "author": {
    "name": "Matthew Sabia",
    "url": "https://x.com/MatthewSabia"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattywhitenz/n8n-nodes-openroutercached.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "node -e \"const fs=require('fs');const path=require('path');fs.mkdirSync('dist/nodes/OpenRouter',{recursive:true});fs.copyFileSync('nodes/OpenRouter/openrouter.svg','dist/nodes/OpenRouter/openrouter.svg');fs.copyFileSync('package.json','dist/package.json');\"",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint ./nodes ./credentials package.json",
    "lintfix": "eslint ./nodes ./credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run postbuild && npm run lint",
    "test": "jest"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OpenRouterCachedApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/OpenRouter/OpenRouterCached.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint": "^8.0.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "jest": "^29.7.0",
    "n8n-core": "^1.59.1",
    "n8n-workflow": "^1.59.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.2.5",
    "typescript": "~4.8.4"
  }
}
